{
    "name": "Slack",
    "app_path": [
        "/usr/share/slack/",
        "/usr/lib/slack/"
    ],
    "icons_path": [
        "/usr/share/slack/resources/",
        "/usr/lib/slack/resources/"
    ],
    "binary": "app.asar",
    "script": "electron",
    "icons": {
        "highlight": {
            "original": "dist/static/slack-taskbar-highlight.ico",
            "theme": "slack-indicator-highlight"
        },
        "indicator": {
            "original": "dist/static/slack-taskbar-rest.ico",
            "theme": "slack-indicator"
        },
        "unread": {
            "original": "dist/static/slack-taskbar-unread.ico",
            "theme": "slack-indicator-unread"
        }
    }
}
